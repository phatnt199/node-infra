{
  "name": "@minimaltech/node-infra",
  "version": "0.2.44",
  "description": "Minimal Technology NodeJS Infrastructure - Loopback 4 Framework",
  "keywords": [
    "web",
    "restful",
    "rest",
    "api",
    "express",
    "auth",
    "security",
    "backend",
    "framework",
    "backend framework",
    "StrongLoop",
    "loopback",
    "loopback-application",
    "infrastructure",
    "minimaltech",
    "minimaltek",
    "mt",
    "infra",
    "nodejs",
    "nodejs framework"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./@loopback/authentication": "./dist/base/loopback/authentication/index.js",
    "./@loopback/authorization": "./dist/base/loopback/authorization/index.js",
    "./@loopback/boot": "./dist/base/loopback/boot/index.js",
    "./@loopback/core": "./dist/base/loopback/core/index.js",
    "./@loopback/http-server": "./dist/base/loopback/http-server/index.js",
    "./@loopback/metadata": "./dist/base/loopback/metadata/index.js",
    "./@loopback/repository": "./dist/base/loopback/repository/index.js",
    "./@loopback/rest": "./dist/base/loopback/rest/index.js",
    "./@loopback/security": "./dist/base/loopback/security/index.js",
    "./@loopback/service-proxy": "./dist/base/loopback/service-proxy/index.js",
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "sh ./scripts/build.sh",
    "build:watch": "lb-tsc --watch",
    "clean": "sh ./scripts/clean.sh",
    "eslint": "eslint --report-unused-disable-directives .",
    "lint": "pnpm eslint && pnpm prettier:cli",
    "lint:fix": "pnpm eslint --fix && pnpm prettier:fix",
    "prettier:cli": "prettier \"**/*.{js,ts}\" -l",
    "prettier:fix": "pnpm prettier:cli --write",
    "rebuild": "sh ./scripts/rebuild.sh no-version",
    "rebuild:major": "sh ./scripts/rebuild.sh major",
    "rebuild:minor": "sh ./scripts/rebuild.sh minor",
    "rebuild:patch": "sh ./scripts/rebuild.sh patch",
    "pretest:dev": "pnpm run rebuild",
    "test:dev": "NODE_ENV=development mocha -r dotenv-flow/config -r source-map-support/register --timeout 60000 --exit dist/__tests__/index.js",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phatnt199/node-infra.git"
  },
  "author": "Minimal Technology <developer@miminaltek.com>",
  "bugs": {
    "url": "https://github.com/phatnt199/node-infra/issues"
  },
  "homepage": "https://github.com/phatnt199/node-infra/wiki",
  "license": "MIT",
  "files": [
    "README.md",
    "LICENSE",
    "dist",
    "!/**/tests",
    "!/**/dist/tests",
    "!/**/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.11.2",
    "@grpc/proto-loader": "^0.7.13",
    "@loopback/authentication": "^11.0.6",
    "@loopback/authentication-jwt": "^0.15.6",
    "@loopback/authentication-passport": "^7.0.6",
    "@loopback/authorization": "^0.15.6",
    "@loopback/boot": "^7.0.6",
    "@loopback/core": "^6.1.3",
    "@loopback/http-server": "^6.0.6",
    "@loopback/metadata": "^7.0.6",
    "@loopback/repository": "^7.0.6",
    "@loopback/rest": "^14.0.6",
    "@loopback/rest-crud": "^0.18.6",
    "@loopback/rest-explorer": "^7.0.6",
    "@loopback/security": "^0.11.6",
    "@loopback/service-proxy": "^7.0.6",
    "@node-oauth/oauth2-server": "^5.2.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@socket.io/redis-emitter": "^5.1.0",
    "axios": "^1.7.7",
    "bullmq": "^5.13.0",
    "casbin": "^5.30.0",
    "cron": "^3.1.7",
    "dayjs": "^1.11.13",
    "dotenv-flow": "4.1.0",
    "ejs": "^3.1.10",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "minio": "^8.0.1",
    "mocha": "^10.7.3",
    "mqtt": "^5.10.1",
    "multer": "1.4.5-lts.1",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "winston": "^3.14.2",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^11.0.6",
    "@loopback/testlab": "^7.0.6",
    "@minimaltech/eslint-node": "^0.1.15",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "^4.17.7",
    "@types/mocha": "^10.0.8",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.16.5",
    "eslint": "^8.57.1",
    "husky": "^9.1.6",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2"
  }
}
